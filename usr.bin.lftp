#include <tunables/global>

/usr/bin/lftp {
  #include <abstractions/apache2-common>
  #include <abstractions/base>



  /etc/inputrc r,
  /etc/lftp.conf r,


  owner /home/*/.local/share/lftp/* rw,
  owner /home/*/[^.]*/[^.]* rw,
  owner /home/*/[^.]* rw,

  deny /home/*/.ssh/ mrwkl,
  deny /home/*/.ssh/* mrwkl,

  /usr/bin/lftp mr,

}
